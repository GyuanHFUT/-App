<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.listening.mapper.BlankMapper">
    <select id="selectAllBlank" resultType="com.listening.domain.Blank">
        SELECT *
        from blank;
    </select>

    <select id="selectBlankOfNum" resultType="java.lang.Integer">
        SELECT COUNT(*)
        FROM blank;
    </select>

    <select id="selectBlankOfMin" resultType="java.lang.Integer">
        SELECT MIN(listen_id)
        FROM blank;
    </select>

</mapper>
